{
  "name": "lint-suite",
  "version": "1.3.1",
  "description": "Reusable ESLint configurations",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/F0rty-Tw0/lint-suite.git",
    "directory": "packages/lint-suite"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "lint",
    "typescript",
    "jest",
    "angular",
    "storybook",
    "json",
    "rxjs",
    "vitest",
    "playwright"
  ],
  "license": "MIT",
  "author": "Artiom Tofan artiomtofancv@gmail.com",
  "type": "module",
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./index.d.ts",
  "dependencies": {
    "@nx/eslint-plugin": "22.5.0",
    "@smarttools/eslint-plugin-rxjs": "1.0.22",
    "@stylistic/eslint-plugin": "5.8.0",
    "@typescript-eslint/utils": "8.55.0",
    "@vitest/eslint-plugin": "1.6.7",
    "angular-eslint": "^21.2.0",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-jest": "29.14.0",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-playwright": "2.5.1",
    "eslint-plugin-storybook": "10.2.8",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-boundaries": "^5.4.0",
    "globals": "17.3.0",
    "typescript-eslint": "8.55.0"
  },
  "scripts": {
    "build": "nx build lint-suite",
    "patch": "pnpm version patch",
    "minor": "pnpm version minor",
    "major": "pnpm version major",
    "publish": "pnpm publish --access public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    }
  },
  "files": [
    "./**",
    "!**/*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  }
}
