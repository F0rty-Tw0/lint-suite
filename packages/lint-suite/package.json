{
  "name": "lint-suite",
  "version": "1.0.1",
  "description": "Reusable ESLint configurations",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/F0rty-Tw0/lint-suite.git",
    "directory": "packages/lint-suite"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "lint",
    "typescript",
    "jest",
    "angular",
    "storybook",
    "json"
  ],
  "license": "MIT",
  "author": "Artiom Tofan artiomtofancv@gmail.com",
  "type": "module",
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./index.d.ts",
  "dependencies": {
    "eslint": "^9.23.0",
    "@nx/eslint-plugin": "^20.6.4",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-json": "^4.0.1",
    "@smarttools/eslint-plugin-rxjs": "^1.0.18",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-import": "^2.31.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "typescript-eslint": "^8.29.0",
    "@nx/devkit": "^20.6.4"
  },
  "scripts": {
    "build": "nx build lint-suite",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major",
    "publish": "npm publish --access public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    }
  },
  "files": [
    "./**",
    "!**/*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  }
}
