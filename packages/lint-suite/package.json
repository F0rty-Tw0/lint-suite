{
  "name": "lint-suite",
  "version": "1.0.10",
  "description": "Reusable ESLint configurations",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/F0rty-Tw0/lint-suite.git",
    "directory": "packages/lint-suite"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "lint",
    "typescript",
    "jest",
    "angular",
    "storybook",
    "json",
    "rxjs",
    "vitest",
    "playwright"
  ],
  "license": "MIT",
  "author": "Artiom Tofan artiomtofancv@gmail.com",
  "type": "module",
  "main": "./index.cjs",
  "module": "./index.js",
  "types": "./index.d.ts",
  "dependencies": {
    "eslint": "^9.38.0",
    "@nx/eslint-plugin": "^21.6.5",
    "eslint-plugin-jest": "29.0.1",
    "eslint-plugin-json": "^4.0.1",
    "@smarttools/eslint-plugin-rxjs": "1.0.22",
    "eslint-plugin-storybook": "9.1.13",
    "eslint-plugin-import-x": "4.16.1",
    "@stylistic/eslint-plugin": "5.5.0",
    "eslint-import-resolver-typescript": "4.4.4",
    "@typescript-eslint/utils": "8.46.2",
    "typescript-eslint": "8.46.2",
    "@vitest/eslint-plugin": "1.3.23",
    "eslint-plugin-playwright": "2.2.2",
    "angular-eslint": "^20.4.0",
    "globals": "15.15.0"
  },
  "scripts": {
    "build": "nx build lint-suite",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major",
    "publish": "npm publish --access public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "require": "./index.cjs"
    }
  },
  "files": [
    "./**",
    "!**/*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  }
}
